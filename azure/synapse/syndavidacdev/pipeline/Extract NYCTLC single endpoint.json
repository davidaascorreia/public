{
	"name": "Extract NYCTLC single endpoint",
	"properties": {
		"activities": [
			{
				"name": "Copy HTTP to ADLS by parameters",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "http_nyctlc",
						"type": "DatasetReference",
						"parameters": {
							"relativeUrl": {
								"value": "@pipeline().parameters.relativeUrl",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "syndavidacdev_data",
						"type": "DatasetReference",
						"parameters": {
							"directory": {
								"value": "@pipeline().parameters.directory",
								"type": "Expression"
							},
							"file": {
								"value": "@pipeline().parameters.file",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"relativeUrl": {
				"type": "string",
				"defaultValue": "misc/taxi+_zone_lookup.csv"
			},
			"directory": {
				"type": "string",
				"defaultValue": "raw/nyctlc/misc"
			},
			"file": {
				"type": "string",
				"defaultValue": "taxi+_zone_lookup.csv"
			}
		},
		"folder": {
			"name": "99 Utilities"
		},
		"annotations": []
	}
}