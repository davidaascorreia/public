						"value": "[parameters('FileSystemNOOSLC9VH533_properties_typeProperties_host')]"
					},
					"FileSystemNOOSLC9VH533_properties_typeProperties_userId": {
						"value": "[parameters('FileSystemNOOSLC9VH533_properties_typeProperties_userId')]"
					},
					"HttpServer2_properties_typeProperties_url": {
						"value": "[parameters('HttpServer2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
